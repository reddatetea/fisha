   "source": [
    "def yield_test(n):\n",
    "    for i in range(n):\n",
    "        yield call(i)       #使用yield语句\n",
    "        print(\"i=\",i)       #做一些其它的事情\n",
    "    print(\"do something.\")\n",
    "    print(\"end.\")\n",
    "def call(i):\n",
    "    return i*2\n",
    "\n",
    "#使用for循环，它使用了next()方法\n",
    "for i in yield_test(5):\n",
    "    print(i,\",\")"
   ]
