    "import winreg\n",
    "import os\n",
    "def get_desktop():  \n",
    "    key = winreg.OpenKey(winreg.HKEY_CURRENT_USER,r'Software\\Microsoft\\Windows\\CurrentVersion\\Explorer\\Shell Folders')  \n",
    "    return winreg.QueryValueEx(key, \"Desktop\")[0]\n",
    "path = get_desktop()\n",
    "files = os.listdir(path)\n",
    "print(files)"
